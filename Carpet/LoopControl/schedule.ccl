# Schedule definitions for thorn LoopControl

SCHEDULE lc_setup AT startup BEFORE Driver_Startup
{
  LANG: C
} "Set up LoopControl"

SCHEDULE lc_statistics_maybe AT analysis
{
  LANG: C
  OPTIONS: meta
} "Output LoopControl statistics"

SCHEDULE lc_statistics AT terminate
{
  LANG: C
  OPTIONS: meta
} "Output LoopControl statistics"
