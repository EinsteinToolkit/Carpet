# Schedule definitions for thorn CarpetTracker



SCHEDULE CarpetTracker_SetPositions AT preregrid
{
  LANG: C
  OPTIONS: global
  READS: SphericalSurface::sf_centroid_x, sf_centroid_y, sf_centroid_z
  READS: SphericalSurface::sf_active, sf_valid
  WRITES: CarpetRegrid2::active(everywhere), positions
} "Set positions of refined regions"
